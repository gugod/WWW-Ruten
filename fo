function clickurl(st,from,tos,tov,tourl)
{
	if(tourl != '')
	{
		$.post(
			's000_addmem.php',
			{
				st:st,
				from:from,
				tos:tos,
				tov:tov
			},
			function(){
				location.href = tourl;
			}
		);
	}
		return false;
}
//key頁碼送出
function jumptopage(p)
{
		var totalp = "165";
		var numcheck = /^\d*$/;
		if((p != '' && !numcheck.test(p)) || p == '')
		{
			alert("僅限輸入半形數字！");
			return false;
		}
		if(p > parseInt(totalp))
		{
			alert("您所輸入的頁數不存在！");
			return false;
		}
		location.href="?searchfrom=headbar&k=iPod&t=0&p=" + p;
		return false;
}

